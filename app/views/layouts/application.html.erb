<!DOCTYPE html>
<html>
<head>
  <title>Dailytechdiscuss</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header-row">
  <div class="container">
    <div class="row">
      <!--LOGO-->
      <div class="span3"><a class="brand" href="/"><%= image_tag "logo.png" %></a></div>
      <!-- /LOGO -->

      <!-- MAIN NAVIGATION -->
      <div class="span9">
        <div class="navbar  pull-right">
          <div class="navbar-inner">
            <a data-target=".navbar-responsive-collapse" data-toggle="collapse" class="btn btn-navbar"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></a>
            <div class="nav-collapse collapse navbar-responsive-collapse">
              <ul class="nav">
                <li class="active"><%= link_to "Home", root_path %></li>
                <li><a href="service.html">Services</a></li>
                <li><a href="blog.html">Blog</a></li>
                <li><a href="contact.html">Contact</a></li>
                <li class="dropdown">
                  <a href="about.html" class="dropdown-toggle" data-toggle="dropdown">About <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li><a href="about.html">Company</a></li>
                    <li><a href="about.html">History</a></li>
                    <li><a href="about.html">Team</a></li>
                  </ul>
                </li>
                <%= link_to "Sign Up", signup_path, class: "btn btn-lg btn-primary" %>
              </ul>
            </div>

          </div>
        </div>
      </div>
      <!-- MAIN NAVIGATION -->
    </div>
  </div>
</div>

<%= yield %>



<!--Footer
==========================-->

<footer>
  <div class="container">
    <div class="row">
      <div class="span6">Copyright &copy 2013 Shapebootstrap | All Rights Reserved <br>
        <small>Aliquam tincidunt mauris eu risus.</small>
      </div>
      <div class="span6">
        <div class="social pull-right">
          <a href="#"> <%= image_tag "social/googleplus.png" %></a>
          <a href="#"> <%= image_tag "social/twitter.png" %></a>
          <a href="#"> <%= image_tag "social/dribbble.png" %></a>
          <a href="#"> <%= image_tag "social/rss.png" %></a>
        </div>
      </div>
    </div>
  </div>
</footer>

<!--/.Footer-->


</body>
</html>
